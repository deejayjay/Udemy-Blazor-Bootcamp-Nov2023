<div class="row">
    <h4 class="text-primary py-3 col-12">Grand Child Component</h4>
    <p>There is a message - @MessageForGrandChild</p>
    <p>The lucky number is - @LuckyNoForGrandChild</p>
</div>

@code {
    [CascadingParameter(Name = "Message")]
    public string? MessageForGrandChild { get; set; }

    [CascadingParameter(Name ="LuckyNumber")]
    public string? LuckyNoForGrandChild { get; set; }
}
